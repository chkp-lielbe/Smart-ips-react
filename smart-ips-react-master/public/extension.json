{
  "extension": {
    "name": "Smart Ips Configuration",
    "product-url": "https://lielberniker.github.io/smart-ips-react/",
    "provider": "lielbe@checkpoint.com",
    "version": "1.3",
    "extension-version-requirements": {
      "minimum-version": "1.3"
    } 
  },
  "locations": [
    {
      "location": "details-pane",
      "relevant-types": ["gateways"],
      "ui-element": {
        "type": "tab",
        "caption": "Smart IPS",
        "tooltip": "Update Smart Ips configuration",
        "action": {
          "details-level": "full",
          "method": "get",
          "trigger-id": "gateways-details-pane",
          "url": "index.html"
        }
      }
    }
  ],
  "requested-permissions": {
    "client": ["get-read-only-session", "run-read-only-commands"],
    "pre-approved-commands": ["run-script"],
    "resource-urls": ["https://cdnjs.cloudflare.com/ajax/libs/vis/4.21.0/"]
  }
}